{
  "name": "@tickethub/frontend",
  "license": "MIT",
  "repository": "https://github.com/TylerWasniowski/tickethub/tree/master/packages/frontend",
  "bugs": "https://github.com/TylerWasniowski/tickethub/issues",
  "version": "0.0.1",
  "main": "lib/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.prod.babel.js --progress --colors",
    "dev": "cross-env NODE_ENV=development webpack-dashboard --color blue -- webpack-dev-server --config ./webpack/webpack.config.devserver.babel.js",
    "lint": "eslint './**/*.{js,jsx}'",
    "lint-fix": "yarn run lint --fix",
    "postlint-fix": "prettier --ignore-path .eslintignore --write './**/*.{js,jsx}'",
    "lint-styles": "stylelint --ignore-path .eslintignore '**/*.{css,scss}' --syntax scss",
    "lint-styles-fix": "yarn run lint-styles --fix",
    "postlint-styles-fix": "prettier --ignore-path .eslintignore '**/*.{css,scss,less}'",
    "test": "jest"
  },
  "dependencies": {
    "cross-env": "^5.2.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "forever": "^0.15.3"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-react-constant-elements": "^7.0.0",
    "@babel/plugin-transform-react-inline-elements": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.14",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.6.1",
    "css-loader": "^0.28.7",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.1",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-security": "^1.4.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.1",
    "jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.2",
    "prettier-package-json": "^2.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-hot-loader": "^4.3.11",
    "react-router": "^4.3.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.0",
    "stylelint": "^9.4.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0",
    "title-case": "^2.1.1",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^0.6.2",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.1",
    "webpack-dashboard": "^2.0.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.4",
    "write-file-webpack-plugin": "^4.3.2",
    "yarn": "^1.9.4"
  },
  "devEngines": {
    "yarn": ">=1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx}": [
        "eslint --fix",
        "prettier --ignore-path .eslintignore --write",
        "git add"
      ],
      "{*.json,.{babelrc,eslintrc,prettierrc,stylelintrc}}": [
        "prettier --ignore-path .eslintignore --parser json --write",
        "git add"
      ],
      "*.{css,scss}": [
        "prettier --ignore-path .eslintignore --write",
        "git add"
      ],
      "*.{yml,less,md}": [
        "prettier --ignore-path .eslintignore --write",
        "git add"
      ],
      "package.json": [
        "prettier-package-json --write",
        "git add"
      ]
    }
  },
  "productName": "TicketHub"
}
